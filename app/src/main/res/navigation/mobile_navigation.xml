<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/home_navigation">

    <include app:graph="@navigation/home_navigation" />

    <include app:graph="@navigation/generator_navigation"
        android:label="@string/menu_generator"/>

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.github.ncliff.passet.presentation.activitys.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_bookmarks"
        tools:layout="@layout/fragment_slideshow" />

    <include app:graph="@navigation/settings_navigation" />

</navigation>